{
  "expo": {
    "name": "Venum",
    "slug": "venum",
    "version": "1.3.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "br.com.alphasoftware.venum",
      "buildNumber": "1.3.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR code tags to insert products into cart",
        "NSLocationWhenInUseUsageDescription": "The app uses the user's approximate location to record where sales were made"
      }
    },
    "android": {
      "package": "br.com.alphasoftware.venum",
      "userInterfaceStyle": "automatic",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}